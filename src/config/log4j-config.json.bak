{
  "appenders": {
    "file": {
      "type": "file",
      "filename": "log/debug.log",
      "maxLogSize": 10485760,
      "backups": 1
    },
    "dateFile": {
      "type": "dateFile",
      "filename": "log/out.log",
      "pattern": "MM-dd.log"
    }
  },
  "categories": {
    "default": {
      "appenders": [
        "file",
        "dateFile"
      ],
      "level": "warn"
    }
  }
}